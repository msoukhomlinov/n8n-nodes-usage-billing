# Cache configuration for n8n node development
cache_version: 1

# Increase cache size for better performance with node_modules
max_cache_size_mb: 2048

# Cache specific file types relevant to n8n development
cached_extensions:
  - .ts
  - .js
  - .json
  - .md
  - .yml
  - .yaml

# Exclude test and build artifacts from caching
exclude_patterns:
  - "**/*.test.ts"
  - "**/*.spec.ts"
  - "**/dist/*"
  - "**/build/*" 